{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/llms-full.md",
      "destination": "/llms-full.txt"
    }
  ],
  "installCommand": "git pull --unshallow https://github.com/Bernankez/blog.git master && pnpm install"
}
